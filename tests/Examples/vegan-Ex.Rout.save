> ## Function to get equal area partitions of the mite data
> cutter <- function (x, cut = seq(0, 10, by = 2.5)) {
+     out <- rep(1, length(x))
+     for (i in 2:(length(cut) - 1))
+         out[which(x > cut[i] & x <= cut[(i + 1)])] <- i
+     return(as.factor(out))}
> ## The hierarchy of sample aggregation
> levsm <- data.frame(
